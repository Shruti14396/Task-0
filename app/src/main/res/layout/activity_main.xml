                android:layout_marginRight="8dp"
                android:background="@drawable/edittext_border"
                android:ems="10"
                android:textSize="14dp"
                android:textColor="#454545"
                android:paddingTop="5dp"
                android:paddingLeft="5dp"
                android:gravity="start|top"
